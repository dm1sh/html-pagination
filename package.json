{
  "name": "html-pagination",
  "version": "1.0.0",
  "description": "Package for html document pagination to fit container with fixed width",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "./lib/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest --config jestconfig.json --passWithNoTests",
